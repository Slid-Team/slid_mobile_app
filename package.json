{
  "name": "slid",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env react-native run-android --appIdSuffix=dev",
    "android:dev": "ENVFILE=.env.dev react-native run-android --appIdSuffix=dev",
    "android:stage": "ENVFILE=.env.stage react-native run-android  --appIdSuffix=test",
    "android:build:dev": "ENVFILE=.env.dev cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "android:build:stage": "ENVFILE=.env.stage  cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "android:build:production": "ENVFILE=.env.production cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "ios": "react-native run-ios --scheme 'Dev'",
    "ios:dev": "react-native run-ios --scheme 'Dev'",
    "ios:stage": "react-native run-ios --scheme 'Stage'",
    "ios:release": "react-native run-ios --scheme 'Release'",
    "ios:ipad13": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (5th generation)\"",
    "ios:ipad11": "react-native run-ios --simulator=\"iPad Pro (11-inch) (3rd generation)\"",
    "dev": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-config": "^1.4.6",
    "react-native-splash-screen": "^3.3.0",
    "react-native-webview": "^11.22.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
